{
"name": "Googly Eyes",
	"version": "0.2",
	"manifest_version": 2,
	"description": "Add silly googly eyes to web pages",
	"background": {
		"scripts": ["js/background.js"]
	},
	"browser_action": {
		"name": "Add eyes to page!",
		"default_icon": "small_icon.png",
		"icons": [
			"small_icon.png"
			]
	},
	"options_page": "options.html",
	"permissions" : [
		"tabs",
		"storage",
		"http://*/*", 
		"https://*/*"
	],
	"web_accessible_resources": [
		"css/images/ui-icons_222222_256x240.png"
	]
}
